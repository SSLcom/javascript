    "url": "https://github.com/sslcom/javascript/issues"
  },
  "homepage": "https://github.com/sslcom/javascript",
  "devDependencies": {
    "babel-tape-runner": "^1.3.1",
    "eslint": "^3.1.1",
    "eslint-find-rules": "^1.11.1",
    "eslint-plugin-import": "^1.11.0",
    "tape": "^4.6.0",
    "in-publish": "^2.0.0"
